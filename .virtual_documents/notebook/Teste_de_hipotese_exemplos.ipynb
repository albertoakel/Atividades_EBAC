notas=notas_0
muo=7
ho=7


#Calcular média e desvio das amostas
n=len(notas)
x_hat=np.mean(notas)
s=np.std(notas,ddof=1)


t=(x_hat-ho)/(s/np.sqrt(n))
df=n-1

#caso 1
p_valor_e = ts.cdf(t, df)
#caso 2
p_valor_d = 1-ts.cdf(t, df)
#caso 3
