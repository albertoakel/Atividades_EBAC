notas_0=[6.5, 7.2, 6.8, 7.5, 6.9, 7.1, 6.7, 7.4, 6.6, 7.1];
notas_1=[2, 3, 6.8, 7.5, 5.9, 4.1, 6.7, 7.4, 6.6, 7.1];  # sig menor
